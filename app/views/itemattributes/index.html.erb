<h1>Listing itemattributes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @itemattributes.each do |itemattribute| %>
  <tr>
    <td><%= itemattribute.name %></td>
    <td><%= itemattribute.description %></td>
    <td><%= link_to 'Show', itemattribute %></td>
    <td><%= link_to 'Edit', edit_itemattribute_path(itemattribute) %></td>
    <td><%= link_to 'Destroy', itemattribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Itemattribute', new_itemattribute_path %>
